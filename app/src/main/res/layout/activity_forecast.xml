<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_forecast"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.teamprobulk.probulk.probulk.AddActivity"
    >

    <Button
        android:text="@string/back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/buttonBackMod"
        android:onClick="forecastStop"
        android:textColor="@color/colorPrimaryDark"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/button7" />

    <TextView
        android:text="Datums2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewDate2"
        android:layout_alignBaseline="@+id/editText2"
        android:layout_alignBottom="@+id/editText2"
        android:layout_alignStart="@+id/textViewDate1"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@color/colorPrimaryDark"/>

    <TextView
        android:text="Datums3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewDate3"
        android:layout_alignBaseline="@+id/editText3"
        android:layout_alignBottom="@+id/editText3"
        android:layout_alignStart="@+id/textViewDate2"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@color/colorPrimaryDark"/>

    <TextView
        android:text="Datums4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewDate4"
        android:layout_alignBaseline="@+id/editText4"
        android:layout_alignBottom="@+id/editText4"
        android:layout_alignStart="@+id/textViewDate3"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@color/colorPrimaryDark"/>

    <TextView
        android:text="Datums5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewDate5"
        android:layout_alignBaseline="@+id/editText5"
        android:layout_alignBottom="@+id/editText5"
        android:layout_alignStart="@+id/textViewDate4"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@color/colorPrimaryDark"/>

    <TextView
        android:text="Datums7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewDate7"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@color/colorPrimaryDark"
        android:layout_alignBaseline="@+id/editText7"
        android:layout_alignBottom="@+id/editText7"
        android:layout_alignStart="@+id/textViewDate6" />

    <TextView
        android:text="Datums6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewDate6"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@color/colorPrimaryDark"
        android:layout_alignBaseline="@+id/editText6"
        android:layout_alignBottom="@+id/editText6"
        android:layout_alignStart="@+id/textViewDate5" />

    <TextView
        android:text="@string/salesBulkName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView26"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@color/colorPrimaryDark"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="clicket7@hotmail.com"
        android:gravity="center_vertical|center"
        android:ems="10"
        android:id="@+id/editText8"
        android:textColor="@color/colorPrimaryDark"
        android:layout_above="@+id/button7"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="@string/email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView23"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@color/colorPrimaryDark"
        android:layout_alignBaseline="@+id/editText8"
        android:layout_alignBottom="@+id/editText8"
        android:layout_alignStart="@+id/button7" />

    <Button
        android:text="@string/send"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button7"
        android:onClick="sendInformation"
        android:textColor="@color/colorPrimaryDark"
        android:layout_above="@+id/buttonBackMod"
        android:layout_centerHorizontal="true" />

    <EditText
        android:layout_width="200px"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:gravity="center_vertical|center"
        android:id="@+id/editText2"
        android:textColor="@color/colorPrimaryDark"
        android:layout_above="@+id/editText3"
        android:layout_alignStart="@+id/editText" />

    <EditText
        android:layout_width="200px"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:gravity="center_vertical|center"
        android:id="@+id/editText3"
        android:textColor="@color/colorPrimaryDark"
        android:layout_above="@+id/editText4"
        android:layout_alignStart="@+id/editText2" />

    <EditText
        android:layout_width="200px"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:gravity="center_vertical|center"
        android:id="@+id/editText4"
        android:textColor="@color/colorPrimaryDark"
        android:layout_above="@+id/editText5"
        android:layout_alignStart="@+id/editText3" />

    <EditText
        android:layout_width="200px"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:gravity="center_vertical|center"
        android:id="@+id/editText5"
        android:textColor="@color/colorPrimaryDark"
        android:layout_above="@+id/editText6"
        android:layout_alignStart="@+id/editText4" />

    <EditText
        android:layout_width="200px"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:gravity="center_vertical|center"
        android:id="@+id/editText6"
        android:textColor="@color/colorPrimaryDark"
        android:layout_above="@+id/editText7"
        android:layout_alignStart="@+id/editText5" />

    <EditText
        android:layout_width="200px"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:gravity="center_vertical|center"
        android:id="@+id/editText7"
        android:textColor="@color/colorPrimaryDark"
        android:layout_above="@+id/editText8"
        android:layout_alignStart="@+id/editText6" />

    <TextView
        android:text="Datums1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewDate1"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@color/colorPrimaryDark"
        android:layout_alignBaseline="@+id/editText"
        android:layout_alignBottom="@+id/editText"
        android:layout_alignStart="@+id/textView23" />

    <EditText
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText"
        android:layout_width="200px"
        android:gravity="center_vertical|center"
        android:editable="false"
        android:textColor="@color/colorPrimaryDark"
        android:layout_above="@+id/editText2"
        android:layout_alignStart="@+id/editText8" />

    <Spinner
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/spinner3"
        android:spinnerMode="dialog"
        android:gravity="center_vertical|center|start"
        android:textColor="@color/colorPrimaryDark"
        android:layout_below="@+id/textView26"
        android:layout_alignParentStart="true" />

    <Button
        android:text="@string/reload"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button14"
        android:textColor="@color/colorPrimaryDark"
        android:onClick="getForecast"
        android:layout_alignTop="@+id/editText"
        android:layout_alignParentEnd="true" />

</RelativeLayout>